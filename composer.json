{
    "name": "tomedharris/nova-tree-builder-tool",
    "description": "A Laravel Nova resource tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "~2.0",
        "illuminate/contracts": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Tomedharris\\NovaTreeBuilderTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tomedharris\\NovaTreeBuilderTool\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tomedharris\\NovaTreeBuilderTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
